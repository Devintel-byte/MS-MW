generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Memory {
  id        String    @id @default(uuid())
  name      String
  message   String
  email     String?
  imageUrl  String
  fPhotoUrl String? // New field for composite (image with card canvas) image URL
  approved  Boolean?  @default(false)
  createdAt DateTime? @default(now())
}
